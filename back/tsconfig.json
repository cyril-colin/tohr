{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "../dist-back",
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
    },
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "../node_modules",
    "**/*.spec.ts"
  ]
}
