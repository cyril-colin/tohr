{
  "name": "tohr",
  "version": "1.0.1",
  "description": "An attempt to be a more mobile friendly UI for transmission-daemon",
  "author": "Cyril COLIN",
  "scripts": {
    "back-build": "node_modules/.bin/tsc --build tsconfig-back.json",
    "back-start": "./node_modules/.bin/ts-node-dev --project tsconfig-back.json back/server.ts --config config.sample.json",
    "back-start-vagrant-prod": "cd /tohr && node /tohr/dist-back/server.js --config config.sample.json",
    "ng": "cd front && ng && cd ..",
    "start": "cd front && ng serve --proxy-config proxy.conf.json --host 0.0.0.0 && cd ..",
    "build": "ng build",
    "test": "ng test --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^12.12.26",
    "axios": "^0.19.2",
    "commander": "^3.0.2",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "ngx-filesize": "^2.0.2",
    "node-cache": "^4.2.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~12.12.26",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "ts-node-dev": "^1.0.0-pre.44"
  }
